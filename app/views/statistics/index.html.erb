<p id="notice"><%= notice %></p>

<h1>Statistics</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"> Events </th>
      <th colspan="3"> Likes </th>
      <th colspan="3"> RSVP </th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.title %></td>
        <td><%= event.get_upvotes.size %> </td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>

